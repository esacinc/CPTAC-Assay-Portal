{% use 'swpg_bootstrap_side_nav.twig' %}

<html dir="ltr" lang="en">
<head>
    <meta charset="UTF-8"/>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="title" content="{{ page_title|e }}"/>
    <meta name="viewport" content="width=device-width, maximum-scale=1.0"/>
    {% block meta_tags %}{% endblock %}
    <title>{{ page_title|e }} | Office of Cancer Clinical Proteomics Research</title>
    <link href="//fonts.googleapis.com/css?family=Archivo+Narrow:400,400italic,700,700italic" rel="stylesheet"
          type="text/css"/>
    <link href="//fonts.googleapis.com/css?family=Montserrat:300,400,500,700|Noto+Sans" rel="stylesheet"
          type="text/css"/>
    <link href="/site/library/images/favicon.png" rel="icon" type="image/png"/>
    <link href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/cabazon_works_lib/sidr/stylesheets/jquery.sidr.light.css" rel="stylesheet"
          type="text/css"/>
    <!--link href="//maxcdn.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet" type="text/css"/>-->

    <link href="/site/library/css/application.css" rel="stylesheet" type="text/css"/>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css"/>

    <link href="/site/library/cabazon_works_lib/fontawesome/css/font-awesome.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/system.base.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/system.theme.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/comment.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/date.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/datepicker.1.7.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/field.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/node.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/search.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/extlink.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/ckeditor.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/ctools.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/custom_admin.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/slider.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/occpr_admin.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/mobile.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/submenu.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_works_css/menu_icons.css" rel="stylesheet" type="text/css"/>
    <link href="/site/library/css/cabazon_styles.css" rel="stylesheet" type="text/css"/>


    {% block styles_head %}
        {% for item in css_includes %}
            <link href="{{ item }}" rel="stylesheet" type="text/css"/>
        {% endfor %}
    {% endblock %}
    <script src="//code.jquery.com/jquery-3.2.1.min.js" type="text/javascript"></script>
    <!--<script src="//code.jquery.com/ui/1.12.1/jquery-ui.js" type="text/javascript"></script>-->
    <script src="/site/library/js/cabazon_works_js/foundation.min.js" type="text/javascript"></script>


    <!--<script src="//cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.3.1/jquery.cookie.min.js" type="text/javascript"></script>-->
    <script src="/site/library/cabazon_works_lib/sidr/jquery.sidr.min.js" type="text/javascript"></script>
    <!--<script src="//maxcdn.bootstrapcdn.com/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>-->
    <script src="/site/library/js/cabazon_works_js/analytics.js" type="text/javascript"></script>
    <script src="https://d3js.org/d3.v4.min.js" type="text/javascript"></script>
    <script src="/site/library/js/cabazon_works_js/drupal.js" type="text/javascript"></script>
    <script type="text/javascript">
        window.$ = jQuery;
    </script>
    <script src="/site/library/js/cabazon_works_js/extlink.js" type="text/javascript"></script>
    <script src="/site/library/js/cabazon_works_js/custom.js" type="text/javascript"></script>
    <!--<script src="/site/library/js/cabazon_works_js/foundation.min.js" type="text/javascript"></script>-->
    <script src="/site/library/js/cabazon_works_js/site.js" type="text/javascript"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.6/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js"></script>

    {% block js_head %}
        {% for item in js_top %}
            <script src="{{ item }}" type="text/javascript"></script>
        {% endfor %}
    {% endblock %}

    {% if google_analytics_key_new %}
        <script type="text/javascript">
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', '{{ google_analytics_key_new }}', '{{ google_analytics_key_new_sitename }}');
            ga('send', 'pageview');


        </script>
    {% endif %}
</head>
<body class="html not-front section-assay-portal">
{% if navbar is not empty %}
    {% include navbar %}
{% endif %}
{% if remove_side_nav != true %}
    {% block show_side_nav %}
        {{ parent() }}
    {% endblock %}
{% endif %}

<!--/.row.full-width.header-->


<div class="container">
    <main role="main" class="">
        <div class="">
            <div class="main-content"><a id="main-content"></a>

                {% if remove_side_nav != true %}
                    {% block side_nav %}
                        {{ parent() }}
                    {% endblock %}
                {% endif %}

                <div>
                    <h1>Assay Portal</h1>
                </div>

                {% block content %}{% endblock %}
            </div>
        </div>
    </main>
</div>
{% if site_footer is not empty %}
    {% include site_footer %}
{% endif %}


{% block js_bottom %}
    {% for item in js_includes %}
        <script src="{{ item }}" type="text/javascript"></script>
    {% endfor %}
{% endblock %}
{% block written_js_bottom %}
    {{ parent() }}
{% endblock %}
</body>
</html>
